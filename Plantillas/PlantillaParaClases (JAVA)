package nombre_paquete;

// Cambia el nombre de la clase según sea necesario
public class Calculadora {
    public static void main(String[] args) {
        // Prueba del método sumar
        int resultado = sumar(5, 3);
        System.out.println("Resultado: " + resultado); // Debería mostrar 8
    }

    public static int sumar(int a, int b) {
        // Cambia el nombre y la funcionalidad del método según tu ejercicio
        return a + b; // Retorna la suma de dos números enteros
    }
}
